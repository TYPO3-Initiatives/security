{
    "name": "typo3/cms-security",
    "type": "typo3-cms-framework",
    "description": "The security library of TYPO3.",
    "homepage": "https://typo3.org",
    "license": ["GPL-2.0-or-later"],
    "authors": [{
        "name": "TYPO3 Core Team",
        "email": "typo3cms@typo3.org",
        "role": "Developer"
    }],
    "config": {
        "sort-packages": true
    },
    "require": {
        "typo3/cms-core": "10.0.*@dev",
        "typo3/cms-extbase": "10.0.*@dev",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "typo3/testing-framework": "^5.0"
    },
    "conflict": {
        "typo3/cms": "*"
    },
    "replace": {
        "security": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "security",
            "Package": {
                "partOfFactoryDefault": true
            }
        }
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\CMS\\Backend\\Policy\\": "Classes/Backend/Policy",
            "TYPO3\\CMS\\Core\\Policy\\": "Classes/Core/Policy",
            "TYPO3\\CMS\\Security\\Policy\\": "Classes/Policy"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\CMS\\Security\\Tests\\": "Tests/"
        }
    }
}